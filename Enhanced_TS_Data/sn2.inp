&GLOBAL
  PRINT_LEVEL LOW
  WALLTIME 240000
   PROJECT ch3f
!  RUN_TYPE WFN_OPT
!   RUN_TYPE GEO_OPT
  RUN_TYPE MD
   SEED 300
&END GLOBAL

!&EXT_RESTART
!EXTERNAL_FILE RESTART
!RESTART_POS T
!RESTART_VEL T
!RESTART_THERMOSTAT T
!&END EXT_RESTART

&MOTION

  &MD
   COMVEL_TOL 0.000005
   ENSEMBLE NVT
   STEPS 3000000 
   TIMESTEP 0.5
   TEMPERATURE 1200.0
!   TEMP_TOL 40

    &THERMOSTAT
      TYPE CSVR
      &CSVR
        TIMECON 100.
      &END CSVR
    &END THERMOSTAT
  &END MD
 
  &FREE_ENERGY
    &METADYN
       USE_PLUMED .TRUE.
       PLUMED_INPUT_FILE ./plumed.dat
    &END METADYN
  &END FREE_ENERGY

  &PRINT
   &TRAJECTORY
     FORMAT XYZ
     &EACH
       MD 1000
     &END EACH
   &END TRAJECTORY
   &VELOCITIES 
    FORMAT XYZ
    &EACH
      MD 1000
    &END EACH
   &END VELOCITIES
   &RESTART
    &EACH
     MD 1000000
    &END EACH
   &END RESTART
  &END PRINT


&END MOTION



&FORCE_EVAL
  METHOD Quickstep
  &DFT
   CHARGE -1
    &QS
      METHOD PM6
      !METHOD AM1
      &SE
      &END SE
    &END QS
    &SCF
      SCF_GUESS ATOMIC
      EPS_SCF 1.0E-5
      MAX_SCF 50
       &OUTER_SCF
          EPS_SCF 1.0E-7
          MAX_SCF 500
       &END
    &END SCF
  &END DFT


  &SUBSYS
    &CELL
      ABC 10.0 10.0 10.0
      PERIODIC NONE
    &END CELL

    &KIND H
      BASIS_SET DZVP-MOLOPT-SR-GTH-q1
      POTENTIAL GTH-PBE-q1
      ELEMENT H
    &END KIND

    &KIND C
      BASIS_SET  DZVP-MOLOPT-SR-GTH-q4
      POTENTIAL GTH-PBE-q4
      ELEMENT C
    &END KIND

    &KIND F
      BASIS_SET  DZVP-MOLOPT-SR-GTH-q7
      POTENTIAL GTH-PBE-q7
      ELEMENT F
    &END KIND

    &KIND Cl
      BASIS_SET  DZVP-MOLOPT-SR-GTH-q7
      POTENTIAL GTH-PBE-q7
      ELEMENT Cl
    &END KIND


    &COORD
C   5.00000   5.00000   5.00000
F   5.00000   5.00000   2.00054
H   4.20083   5.69371   4.63796
H   4.79882   3.96104   4.63796
H   6.00035   5.34525   4.63796
Cl  5.00000   5.00000   6.80000
     &END COORD
   &END SUBSYS

&END FORCE_EVAL
